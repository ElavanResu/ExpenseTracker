{
    "version": "0.2.0",
    "configurations": [
        {
            "cwd":"${workspaceRoot}",
            "name": "Debug Android",
            "program": "${workspaceRoot}/.vscode/launchReactNative.js",
            "type": "reactnative",
            "request": "launch",
            "platform": "android",
            "sourceMaps": true,
            "outDir": "${workspaceRoot}/.vscode/.react",
            "runtimeArgs": [
                "--harmony",
                "--no-deprecation"
            ],
        },
       
    ]
}